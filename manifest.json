{
  "manifest_version": 2,
  "name": "DeleteRow",
  "description": "Toolbar button to delete a row in a table.",
  "version": "1.0",
  "author": "C-E",
  "icons": {
      "16": "delrow.svg",
      "24": "delrow.svg",
      "48": "delrow.svg",
      "96": "delrow.svg"
  },
   "applications": {
      "gecko": {
         "id": "DelRow@pharqcon.com",
         "strict_min_version": "68.0"
      }
   },
  "experiment_apis": {
    "myapi": {
      "schema": "schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "myapi"
          ]
        ],
        "script": "implementation.js"
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "compose_action": {
    "default_title": "DelRow",
    "default_icon": "delrow.svg"
  }
}
